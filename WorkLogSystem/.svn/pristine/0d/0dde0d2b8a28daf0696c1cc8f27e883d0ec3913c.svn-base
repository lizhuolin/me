@charset "utf-8";
/* CSS Document */

body { font-family:"冬青黑体";-webkit-font-family:"冬青黑体";-moz-font-family:"冬青黑体"; color:#646464; font-size:14px; text-align:left; max-width:1920px; min-width:320px; _width:expression(this.scrollWidth > 1920 ? "1920px" : (this.scrollWidth < 320? "320px" : "auto")); margin: 0 auto; letter-spacing:1px;}
li{ list-style:none;}
*{ margin:0; padding:0; box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;  }
a{ text-decoration:none; cursor:pointer; color:#666;}
a:active, a:hover {
	outline: 0; text-decoration:none;
}
:focus{ outline:0;}

button, input, optgroup, select, textarea {
	color: inherit;
	font: inherit;
	margin: 0
}
button, html input[type=button], input[type=reset], input[type=submit] {
	-webkit-appearance: button;
	cursor: pointer
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
td, th {
	padding: 0 10px; text-align:center; border:1px solid #ccc;
}
img {
	vertical-align: middle;
	border: 0
}

header,aside{ display:block;}
i,em{ font-style:normal;}
@-ms-viewport {width:device-width}

.clear{ clear:both;}
.fl { float:left; }
.fr {float:right; }
.tl { text-align:left; }
.tr { text-align:right; }
.tc { text-align:center; }
.font0{ font-size:0;}
.hide{ display:none !important;}

/*字形图标*/
@font-face {font-family: "iconfont";
  src: url('../fonts/iconfont.eot'); /* IE9*/
  src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../fonts/iconfont.woff') format('woff'), /* chrome、firefox */
  url('../fonts/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('../fonts/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shang:before { content: "\e600"; }
.icon-jiantouzuo:before { content: "\e608"; font-size:30px; }
.icon-jiantouyou:before { content: "\e609";  font-size:30px;}
.icon-you:before { content: "\e601"; }
.icon-zuo:before { content: "\e602"; }
.icon-caretright:before { content: "\e603"; font-size:30px;}
.icon-caretleft:before { content: "\e604"; font-size:30px;}
.icon-circle_down_on:before { content: "\e605"; }
.icon-212102:before { content: "\e606"; }
.icon-2121021:before { content: "\e607"; }



header{ width:100%; height:120px; background:#658bb2; color:#fff; position:fixed; top:0; z-index:9;}
header span{ display:inline-block; vertical-align:middle;}
header .head_sp1{ width:20%; height:120px; background:url(../images/logo.png) center 50% no-repeat; background-size:50%;}
header .head_sp2{ width:59.5%; text-align:center; font-size:20px; line-height:120px;}
header .head_sp3{ width:20%; text-align:right; line-height:30px; font-size:14px; padding-top:30px; padding-right:50px;}

aside{ width:280px; background:#fff; color:#333; position:fixed; top:120px; left:0; z-index:9; overflow:auto; border-right:20px solid #f0f0f0;}
aside a{ color:#333;}
.per_info{ width:100%; padding:10px; font-size:12px; border-bottom:1px solid #ccc;}
.photo{ width:90px; height:90px; margin:0 auto; border:4px solid #ccc; border-radius:50%;}
.photo img{ width:100%; height:100%; border-radius:50%;}
.name{ width:100%; text-align:center; color:#aabd6e; font-size:14px;}

.menu>li{ width:100%; line-height:40px; border-bottom:1px solid #ccc;}
.menu>li img{ width:22px; margin-right:5px;}
.menu>li a{ display:block; padding:0 20px;}
.menu>li i{ font-size:15px; color:#666;}

.sub_menu>li{ width:100%; line-height:40px; padding-left:40px; font-size:12px; border-top:1px solid #ccc;}
.sub_menu>li.select{ background:#7ba2cb;}

.flex_menu{ display:block; position:fixed; left:260px; top:48%; background:#bfbfbf; width:20px; line-height:60px; border-radius:0 10px 10px 0; opacity:0.5; z-index:99;}
.flex_menu i{ margin-left:-5px;}
.flex_menu:hover{ opacity:1;}

.main{ width:100%; margin-top:120px; padding-left:280px; background:#f0f0f0; padding-bottom:10px;}
.main_top{ min-height:60px; background:#fff; padding:10px 0; overflow:hidden;}
.con_top{ min-width:1080px; padding-left:50px; padding-top:5px; height:75px; overflow:hidden; position:relative;}
.con_top>div{ display:inline-block; vertical-align:top;}
.con_search{ width:850px; line-height:25px;}
.con_search>span{ display:inline-block; margin-bottom:5px; position:relative;}
.con_search>span>input,.con_search>span>select{ width:160px; height:29px; border:1px solid #ccc; padding:0 5px;}
.con_search>span>i{ display:inline-block; width:100px; text-align:right;}
.search_btn>a{ display:block; background:#7ba2cb; color:#fff; padding:5px 0; border-radius:5px; width:70px; text-align:center; margin:5px;}
.search_btn2>a{ display:block; background:#7ba2cb; color:#fff; padding:5px 0; border-radius:5px; width:70px; text-align:center; margin:5px;}
.con_search samp{ margin-right:5px;}

.toggle{ position:absolute; left:890px; bottom:0; display:none;}

.content{ padding:20px 50px; margin:10px 0; background:#fff;}
.con_btn{ width:100%;}
.con_btn a{ display:inline-block; background:#7ba2cb; color:#fff; padding:5px 0; border-radius:5px; width:70px; text-align:center; margin-right:10px; margin-bottom:5px;}

.con_search>span label.error{ font-size:12px; color:red; position:absolute; left:115px; top:30px; z-index:999; background-color:#f8f8f8; padding:0 5px;}
.con_search>span label.success{ display:none !important;}


.con_disc{ margin-top:10px;}
.con_disc>table{ min-width:1080px;}
.con_disc>table tr{ height:28px;}
.con_disc>table th{ white-space:nowrap;}
.con_disc>table td{ max-width:500px; word-wrap:break-word; text-overflow:ellipsis; white-space:nowrap; overflow:hidden; }
.con_disc>table td:hover{ white-space:normal; overflow:auto;}
.con_disc>table tr:nth-child(odd){ background:#ecebf0;}
.con_disc>table a{ display:inline-block; padding:2px 10px; background:#7ba2cb; color:#fff; border-radius:5px; margin:0 5px;}
.con_disc>table td.btn{ text-align:left; white-space:nowrap;}

.main_bottom{ background:#fff; margin-top:10px; min-height:50px;}
.con_bottom{ min-width:1080px; padding:10px 0; padding-left:50px;}
.con_bottom>div{ display:inline-block; vertical-align:middle;}
.bottom_disc{ width:1000px; line-height:25px; text-align:right;}
.bottom_disc span{ display:inline-block; vertical-align:middle; margin-right:10px;}
.bottom_disc span input{ width:50px; border:1px solid #ccc; padding:0 5px;}
.bottom_disc span select{ width:50px; border:1px solid #ccc; height:26px;}


.ord_type{ width:100%; min-width:1080px; padding:10px 0; border-bottom:1px solid #ccc; position:relative;}
.ord_til{ padding:10px;}
.ord_til a{ display:inline-block; width:100px; text-align:center; line-height:30px; background:#ccc; color:#999; border-radius:5px;}
.ord_til a img{ width:20px;}
.ord_til a span{ font-size:15px; vertical-align:middle;}
.ord_til a.select{ background:#7ba2cb; color:#333;}

.ord_pro a{ display:inline-block; width:100px; text-align:center; line-height:30px; background:#ecebf0; margin:10px; border-radius:5px; vertical-align:bottom; position:relative; color:#999;}
.ord_pro a.ord_btn{ width:80px; text-align:center; font-size:40px; line-height:30px; moz-line-heght:22px; height:30px; padding:0; color:#fff;}
.ord_pro a.select{ background:#bfd8f3; color:#666;}

.mian_top_btn{ width:1200px; margin:0 auto;}
.mian_top_btn a{ display:block; background:#7ba2cb; color:#fff; padding:5px 0; border-radius:5px; width:60px; text-align:center; margin-left:30px;}

.tr_mini { position:absolute; top:0; right:0; display:none;
    width: 0;
    height: 0;
    border-top: 10px solid #ccc;
    border-left:10px solid transparent; 
}
.ord_pro a>div.select{border-top: 10px solid #96ecc9;}

.tog_icon{ display:block; width:20px; height:20px; position:absolute; left:-20px; bottom:-5px;}
.ord_type>a.up{ background:url(../images/15_03.png) no-repeat; background-size:100%;}
.ord_type>a.down{ background:url(../images/15_06.png) no-repeat; background-size:100%;}

input:read-only{ background:#eee;}
input:-moz-read-only{ background:#eee;}
input:-webkit-read-only{ background:#eee;}

label.require_lab:before{content:'*'; color:red; font-size:16px;}

form{overflow:hidden;zoom:100%;}
.edit{ padding:10px 0; margin-bottom:60px;}
.edit>ul>li{ padding:10px 0;}
.edit>ul>li>span{ display:inline-block; width:49%; position:relative; vertical-align:top; font-size:0;}
.edit>ul>li>span i{ display:inline-block; width:100px; vertical-align:top; text-align:right; font-size:14px;}
.edit>ul>li>span samp{ font-size:14px; vertical-align:top; margin-right:5px;}
.edit>ul>li>span textarea{ width:180px; height:60px; line-height:20px; overflow:auto;border:1px solid #ccc; resize:none;}
.edit>ul>li>span input,.edit>ul>li>span select,.edit>ul>li>span textarea,.edit>ul>li>span>div{ font-size:14px;}
.edit>ul>li>span input[type="text"],.edit>ul>li>span input[type="number"],.edit>ul>li>span input[type="password"],.edit>ul>li>span select{ width:180px; height:30px; border:1px solid #ccc; padding:0 5px;}
.edit>ul>li>span em{ display:inline-block; width:49.5%; font-size:14px;}
.edit>ul>li>span .prop_sp{ display:inline-block; width:200px; padding:0;}

.sp_role em{ display:block !important;}

.edit>ul>li>span input[type="file"]{ width:90px; position:absolute; left:113px; top:0; opacity:0; cursor:pointer;}
.upload_btn{ display:inline-block; font-size:14px; color:#fff; background:#7ba2cb; width:90px; text-align:center; padding:5px 0; border-radius:5px; margin-right:5px; position:relative;}
.files{margin-left:113px; width:220px; line-height:25px; margin-top:10px;}
.file_name{ border:none !important; width:178px; overflow:hidden;text-overflow:ellipsis; white-space:nowrap; text-decoration:underline;vertical-align:middle; cursor:pointer; background:#fff;}
.file_del{ display:inline-block; width:15px; height:15px; background:url(../images/uploadify-cancel.png) no-repeat; vertical-align:middle; margin-left:5px;}

.upload_btn>input[type="file"]{ left:0 !important;}


.con_btns{ height:30px;}
.con_btns>a{ display:inline-block; background:#ff7b7c; color:#fff; padding:5px 0; border-radius:5px; width:60px; text-align:center; margin-right:10px;}

.edit>ul>li>span label.error{ font-size:12px; color:red; position:absolute; left:115px; top:30px;}
.edit>ul>li>span label.success{ display:none !important;}
.spec_item>label{ font-size:12px; color:red;position:initial !important;}

.edit>ul>li>span img{ width:100px; height:100px; border-radius:50%; position:absolute; left:113px;}

.d_list{ width:180px; height:200px; overflow:auto; background:#fff; border:1px solid #ccc; position:relative; left:113px; z-index:99; font-size:14px !important; display:none; margin-bottom:60px; }
.d_list li{ white-space:nowrap;font-size:14px !important;}

.d_list1{ width:100%; height:300px; overflow:auto; background:#fff; border:1px solid #ccc; position:relative; left:113px; top:-20px; font-size:14px !important; }
.d_list1 li{ white-space:nowrap;font-size:14px !important;}

.d_list2{ width:180px; height:200px; overflow:auto; background:#fff; border:1px solid #ccc; position:fixed; z-index:99; font-size:14px !important; display:none; }
.d_list2 li{ white-space:nowrap;font-size:14px !important;}

.d_list3{ width:180px; height:120px; overflow:auto; background:#fff; border:1px solid #ccc; position:relative; left:100px; top:-20px; font-size:14px !important; }
.d_list3 li{ white-space:nowrap;font-size:14px !important;}

.d_list4{ width:180px; height:120px; overflow:auto; background:#fff; border:1px solid #ccc; position:relative; left:113px; top:-20px; font-size:14px !important; }
.d_list4 li{ white-space:nowrap;font-size:14px !important;}


.edit_btn{ width:100%; height:50px; padding-top:10px; text-align:center; position:fixed; bottom:0; z-index:990; background-color:#f8f8f8; border-top:1px solid #eee;}
.edit_btn>a{ display:inline-block; background:#7ba2cb; border:1px solid #7ba2cb; color:#fff; padding:5px 0; border-radius:5px; width:70px; text-align:center; margin-right:10px;}
.edit_btn>a:nth-child(2){ background-color:#f1f1f1 !important; border:1px solid #dedede !important; color:#333 !important;}

.fix_arrow{ width:30px; height:30px; position:absolute; right:10px; top:40%;}


/*登录*/
