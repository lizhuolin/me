.zc_btn{ width:100%; margin:30px 0;}
.zc_btn a{ display:block; width:100%; text-align:center; line-height:40px; background:#92bde8; border-radius:5px; font-size:18px; color:#fff;}
.lg_bgs{ width:414px; margin:0 auto; height:200px; background:url(../images/bg_img1.png) no-repeat; background-size:100%;}

.list_top{ min-height:60px; background:#ecebf0; padding:20px 0; margin-top:10px;}
.list_bottom{ background:#ecebf0; margin-top:10px; min-height:50px;}

.tb_cover{ width:500px; background:rgba(0,0,0,0.5); padding:20px; padding-left:40px; position:fixed; top:120px; left:35%; z-index:99; display:none;}
.tb_cover span{ display:inline-block; width:32%; font-size:14px; color:#fff; margin-bottom:10px;}
.tb_cover span input{ vertical-align:middle; margin-right:5px;}

.batch_all span{ padding-left:120px;}
input.red_border{ border:1px solid red !important;}

input.mini{ width:50px !important; padding-left:5px;}
input.middle{ width:160px !important; padding-left:5px;}
input.spec_input{ line-height:30px; vertical-align:top; margin-top:4px; margin-right:5px;}

.checkboxLabel{ font-size:14px !important;}

.set{ margin:0 auto; margin-top:20px; padding-left:50px;}
.set span{ display:inline-block; width:19%; margin-bottom:5px;}
.select_all{ font-size:16px; margin-bottom:10px;}

.tabs{ width:100%; padding-top:10px; padding-left:100px; border-bottom:1px solid #ccc;}
.tabs a{ display:inline-block; width:80px; text-align:center; padding:5px; border:1px solid #ccc; border-radius:5px 5px 0 0; margin-right:5px; border-bottom:none;}
.tabs a.select{ background:#7ba2cb; color:#fff;}
.tabs a:hover{ background:#7ba2cb; color:#fff;}


/*滞纳金*/
.rules{ display:inline-block; width:49%; vertical-align:top; margin-bottom:20px;}
.rule_title{ line-height:30px; font-size:16px; color:#333; border-bottom:1px solid #ccc;}
.rule_disc p{ line-height:20px; margin-top:5px;}
.rule_btn a{ display:block; margin-top:20px; width:70px; text-align:center; line-height:30px; font-size:15px; color: #fff; background:#7ba2cb ; border-radius:5px;}

.rule_disc p span{ position:relative;}
.rule_disc p span label.error{ font-size:12px; color:red; position:absolute; left:0; top:20px; white-space:nowrap;}
.rule_disc p span label.success{ display:none !important;}


.in_line{ width:100% !important;}
.in_line>span{ display:inline-block; width:80%;}
.in_line>span>em{ width:16.5% !important; overflow:hidden;text-overflow:ellipsis; white-space:nowrap;}

.tip{ color:red; font-size:12px;}

.tog_title{ width:100%; line-height:40px; color:#fff; background:#7ba2cb; padding:0 10px; border-bottom:1px solid #fff;}
.tog_title span{ font-size:16px;}
.tog_tab i{ color:#fff;}

.se_continent{ width:100%; text-align:center; padding:10px; max-width:800px; margin:0 auto;}
.se_country .coun_list{ width:100%; padding-left:50px; margin-top:20px;}
.se_country .coun_list span{ display:inline-block; width:24%; margin-bottom:5px;}

.edit_cover{ width:100%; height:100%; position:fixed; top:0; left:0; z-index:99999; background:rgba(0,0,0,0.3);}
.edit_cover p{ width:50px; height:50px; margin:0 auto; margin-top:200px;}
.edit_cover p img{ width:100%; height:100%;}

.se_btns{ width:100%; padding:10px; max-width:800px; margin:0 auto; padding-left:8%;}
.se_btns a{ margin-bottom:5px;}


