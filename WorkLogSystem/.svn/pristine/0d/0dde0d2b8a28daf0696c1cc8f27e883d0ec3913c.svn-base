.lg_logo{ width:100%; text-align:center; margin-top:8%; padding-left:80px;}
.lg_logo img{ width:280px;}
.lg_disc{ width:400px; margin:0 auto; padding:50px; padding-bottom:50px; background-color:#85a2c0; color:#fff; font-size:16px;}
