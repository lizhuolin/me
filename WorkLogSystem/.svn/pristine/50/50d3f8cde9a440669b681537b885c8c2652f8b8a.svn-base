package com.poobo.core.enums;

public enum EnumRole {
	SUPERADMIN(0, "超级管理员"), CADMIN(1, "公司管理员"),PUSER(2,"平台用户"),CUSER(3,"公司用户");
	
	private int value;
	private String desc;

	EnumRole(int value, String desc) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}

	public String getDesc() {
		return desc;
	}

	
	
	

}
