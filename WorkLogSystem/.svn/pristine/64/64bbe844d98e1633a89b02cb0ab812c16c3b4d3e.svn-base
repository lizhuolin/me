package com.poobo.task.enums;


/**
 * 任务状态枚举
 * @author zhiliang.lan 2016年3月10日下午6:11:37
 *
 */
public enum EnumTaskStatus {
	N_START(0, "未开始"), ING(1, "进行中"), OVER(2, "已结束");

	private int value;
	private String desc;

	EnumTaskStatus(int value, String desc) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}

	public String getDesc() {
		return desc;
	}

	
	
	

}
