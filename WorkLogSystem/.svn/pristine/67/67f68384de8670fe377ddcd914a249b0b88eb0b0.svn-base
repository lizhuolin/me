@charset "utf-8";
/* CSS Document */

body { font-family:"冬青黑体";-webkit-font-family:"冬青黑体";-moz-font-family:"冬青黑体"; color:#646464; font-size:14px; text-align:left; max-width:1920px; min-width:320px; _width:expression(this.scrollWidth > 1920 ? "1920px" : (this.scrollWidth < 320? "320px" : "auto")); margin: 0 auto; letter-spacing:1px;}
li{ list-style:none;}
*{ margin:0; padding:0; box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;  }
a{ text-decoration:none; cursor:pointer; color:#666;}
a:active, a:hover {
	outline: 0; text-decoration:none;
}
:focus{ outline:0;}

button, input, optgroup, select, textarea {
	color: inherit;
	font: inherit;
	margin: 0
}
button, html input[type=button], input[type=reset], input[type=submit] {
	-webkit-appearance: button;
	cursor: pointer
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
td, th {
	padding: 0 10px; text-align:center; border:1px solid #ccc;
}
img {
	vertical-align: middle;
	border: 0;
	
}
.noticeText img{
    width: 50%; 
}
img#remindImg {
    width: 50%;
}

header,aside{ display:block;}
i,em{ font-style:normal;}
@-ms-viewport {width:device-width}

.clear{ clear:both;}
.fl { float:left; }
.fr {float:right; }
.tl { text-align:left; }
.tr { text-align:right; }
.tc { text-align:center; }
.font0{ font-size:0;}
.hide{ display:none !important;}

/*字形图标*/
@font-face {font-family: "iconfont";
  src: url('../fonts/iconfont.eot'); /* IE9*/
  src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../fonts/iconfont.woff') format('woff'), /* chrome、firefox */
  url('../fonts/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('../fonts/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shang:before { content: "\e600"; }
.icon-jiantouzuo:before { content: "\e608"; font-size:30px; }
.icon-jiantouyou:before { content: "\e609";  font-size:30px;}
.icon-you:before { content: "\e601"; }
.icon-zuo:before { content: "\e602"; }
.icon-caretright:before { content: "\e603"; font-size:30px;}
.icon-caretleft:before { content: "\e604"; font-size:30px;}
