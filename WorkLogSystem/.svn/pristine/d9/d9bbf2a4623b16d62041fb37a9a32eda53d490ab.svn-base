/*进度环*/
#loadedNum {width: 100%; height: 100%; font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;}
* {margin: 0; padding: 0;}
.pj_other_right { z-index: 999; display: table; width:39%; float:right; height:100%}
.pj_other_right>.splash-inner {display: table-cell; text-align: center; vertical-align: middle;}
.pj_other_right .loading-circle {position: relative; width: 100px; height: 100px; margin: auto;}
.pj_other_right .loading-circle>* {display: block; position: absolute; box-sizing: border-box; border-radius: 102px;}
.pj_other_right .loading-circle>canvas.bg {z-index: 1; width: 100%; height: 100%; border: 8px solid #ddd ;}
.pj_other_right .loading-circle>canvas.mask{ margin-top: 15px;width: 80%;height: 80%;background-color: #80dceb;z-index: 2; transform: rotate(-90deg); -webkit-transform: rotate(-90deg); -moz-transform: rotate(-90deg);}
.pj_other_right .loading-circle>canvas.mask1 {margin-top: 15px;width: 80%;height: 80%;background-color: #71b3f0;z-index: 2; transform: rotate(-90deg); -webkit-transform: rotate(-90deg); -moz-transform: rotate(-90deg);}
.pj_other_right .loading-circle>p {z-index: 3;  width: 100%; height: 0%; text-align: center; line-height: 104px; font-size: 23px; font-weight: lighter; color: #000;}



