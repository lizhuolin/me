}
.file {
   position: relative;
    display: inline-block;
    background: #35c7e1;
    border-radius: 100px;
    padding: 4px 12px;
     overflow: hidden; 
    color: #fff;
    text-decoration: none;
    text-indent: 0px;
    line-height: 25px;
    margin-left: 50%;
    margin-top: -6px;
}
.user_form input[type=file] {
    position: absolute;
    font-size: 100px;
    right: 0;
    top: 0;
    opacity: 0;
}
.user_form input[type=file]:hover {
    background: #1E88C7;
    border-color: #78C3F3;
    color: #004974;
    text-decoration: none;
}
.user_form {
	border-top:1px solid #fff;
	border-bottom:1px solid #fff;
	text-align:center;
	padding-bottom:5px;
	 margin: 10% 0;
	background-color:#FFF;
	}
.alter{
    text-align: center;
    margin-top: 30px;
    padding-bottom: 20%;
    background-color: #FFF;
    padding-top: 5%;
}
.alter input{
	font-family:"汉仪中圆简";
	border-radius: 6px;
    color: #005982;
    width: 30%;
    height: 35px;
    background-color: #FFF;
    border-color: #005982;
    border: 1px solid;
}
.user_form span{width:5em;display:block;float:left;line-height:25px;color: #303030; font-family:"冬青黑体简体中文 W3";text-align: left;padding-left: 3%;}

.user_form input [type=file]{
	background:url(../images/liulan.png);
	width:50px;
	height:50px;
}
