<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<%@ taglib prefix="pb" uri="http://http://www.poobo.com//tags/privilege-1.0" %>
<%
String aPath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+request.getContextPath()+"/";
%>
        <div id="footer_wrapper">
    	<footer>
  			<pb:pri privilege="show" module="notice"><div class="message"><a href="<%=aPath%>mobile/notice/mobile_noticeList.action">消息</a></div></pb:pri>
  			<pb:pri privilege="show" module="project"><div class="project"><a href="<%=aPath%>mobile/project/mobile_projectList4mobile.action">项目</a></div></pb:pri>
  			<pb:pri privilege="show" module="log"><div class="log"><a href="<%=aPath%>mobile/log/mobile_log.action">日志</a></div></pb:pri>
  			<pb:pri privilege="show" module="task"><div class="task"><a href="<%=aPath%>mobile/task/mobile_taskList4Mobile.action">任务</a></div></pb:pri>
  			<div class="user"><a href="<%=aPath%>mobile/user/mobile_userList.action">个人</a></div>
  			<div class="clear"></div>
		</footer>

    </div><!--footer_wrapper结束-->